{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:display_context",
    "cases": [
      {
        "when": [
          "gui",
          "fixed",
          "ground"
        ],
        "model": {
          "type": "minecraft:select",
          "property": "minecraft:component",
          "component": "minecraft:custom_name",
          "cases": [
            {
              "when": "Ruby",
              "model": {
                "type": "minecraft:model",
                "model": "minecraft:item/ruby_2d"
              }
            }
          ],
          "fallback": {
            "type": "minecraft:model",
            "model": "minecraft:item/emerald_2d"
          }
        }
      },
      {
        "when": [
          "firstperson_righthand",
          "firstperson_lefthand"
        ],
        "model": {
          "type": "minecraft:select",
          "property": "minecraft:component",
          "component": "minecraft:custom_name",
          "cases": [
            {
              "when": "Ruby",
              "model": {
                "type": "minecraft:model",
                "model": "minecraft:item/ruby_3d_firstperson"
              }
            }
          ],
          "fallback": {
            "type": "minecraft:model",
            "model": "minecraft:item/emerald_3d_firstperson"
          }
        }
      }
    ],
    "fallback": {
      "type": "minecraft:select",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when": "Ruby",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/ruby_3d"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/emerald_3d"
      }
    }
  }
}